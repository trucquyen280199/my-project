{
    "name": "smart-tool",
    "version": "4.1.0",
    "description": "Smart Tool Compiles for Nunjucks Template Engine, Sass, & JS",
    "main": "index.js",
    "repository": "git@github.com:ghinlop/smart-tool.git",
    "author": "Ghin Lop <ghinlop@gmail.com>",
    "license": "MIT",
    "scripts": {
        "dev": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=development webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "hot": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "html": "gulp html",
        "copy": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js --copy"
    },
    "devDependencies": {
        "autoprefixer": "^9.4.10",
        "bootstrap": ">=4.3.1",
        "browser-sync": "^2.26.3",
        "browser-sync-webpack-plugin": "2.0.1",
        "cross-env": "^5.2.0",
        "css-mqpacker": "^7.0.0",
        "font-awesome": "^4.*",
        "gulp": "^4.0.0",
        "gulp-nunjucks-render": "^2.2.2",
        "gulp-plumber": "^1.2.1",
        "gulp-watch": "^5.0.1",
        "jquery": "^3.3.1",
        "laravel-mix": "^4.0.14",
        "lodash": "^4.17.11",
        "popper.js": "^1.14.7",
        "sass": "^1.17.2",
        "sass-loader": "7.*",
        "tailwindcss": "^1.0.4",
        "vue-template-compiler": "^2.6.8"
    },
    "dependencies": {
        "gulp-plumber-notifier": "^0.1.0",
        "laravel-mix-purgecss": "^4.2.0",
        "gulp-beautify": "^3.0.0"
    },
    "browserslist": [
        "last 2 version",
        "ie >= 9",
        "ie_mob >= 10",
        "ff >= 30",
        "chrome >= 34",
        "safari >= 7",
        "opera >= 23",
        "ios >= 7",
        "android >= 4.4",
        "bb >= 10"
    ]
}
